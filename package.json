{
  "name": "chandrian",
  "displayName": "Chandrian",
  "description": "A semantic color theme for VS Code",
  "version": "1.0.0",
  "publisher": "narenranjit",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#262626",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/narenranjit/chandrian-theme.git"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Chandrian Purple",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-purple.json"
      },
      {
        "label": "Chandrian Purple (no italics)",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-purple-no-italics.json"
      },
      {
        "label": "Chandrian Purple (High Contrast)",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-purple-high-contrast.json"
      },
      {
        "label": "Chandrian Blue",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-blue.json"
      },
      {
        "label": "Chandrian Blue (no italics)",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-blue-no-italics.json"
      },
      {
        "label": "Chandrian Blue (High Contrast)",
        "uiTheme": "vs-dark",
        "path": "./dist/chandrian-blue-high-contrast.json"
      }
    ]
  },
  "scripts": {
    "build": "node ./build/compile-themes.js",
    "watch": "node ./build/watch-and-compile.js"
  },
  "author": "Naren Ranjit",
  "license": "ISC",
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "json-templates": "^2.0.0",
    "jsonc-parser": "^2.0.1",
    "prettier": "^2.2.1"
  }
}
